<style>
    /* General Body Styling */
    body {
        font-family: 'Arial', sans-serif;
        background-color: #f4f4f4;
        color: #333;
        margin: 0;
        padding: 0;
    }

    /* Navbar Styling */
    .navbar {
        /*background-color:rgb(144, 174, 167);/* Updated to RGB color */
        background-color:#2c3e50;
        color: #fff;
        padding: 10px 20px;
        display: flex;
        justify-content: space-between;
        align-items: center;
        position: sticky;
        top: 0;
        z-index: 1000;
    }

    .navbar .nav {
        list-style: none;
        margin: 0;
        display: flex;
        align-items: center;
    }

    .navbar .nav li {
        margin: 0 10px;
    }

    .navbar .nav li a {
        color: #fff;
        text-decoration: none;
        font-weight: bold;
    }

    .navbar .nav li a:hover {
        color: #e74c3c;
    }

    /* Sidebar Styling */
    .navbar-aside {
       /* background-color: rgb(144, 174, 167);/* Updated to RGB color */
       background-color: #2c3e50;
        width: 250px;
        height: 100vh;
        position: fixed;
        overflow-y: auto;
        padding: 20px;
        box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1);
    }

    .menu-aside .menu-item {
        margin: 15px 0;
    }

    .menu-aside .menu-link {
        color: #ecf0f1;
        text-decoration: none;
        display: block;
        padding: 10px 15px;
        border-radius: 5px;
        font-size: 16px;
        transition: all 0.3s ease;
    }

    .menu-aside .menu-link:hover,
    .menu-aside .menu-link.active {
        background-color: #1abc9c;
        color: #fff;
    }

    /* Content Styling */
    .main-wrap {
        margin-left: 270px;
        padding: 20px;
    }

    .content-header {
        background-color: #ffffff;
        padding: 20px;
        border-radius: 8px;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .content-title {
        font-size: 24px;
        color: #2c3e50;
    }

    /* Footer Styling */
    footer {
        background-color: #2c3e50;
        color: #fff;
        padding: 10px 20px;
        text-align: center;
        position: relative;
        bottom: 0;
        width: 100%;
    }

    footer a {
        color: #1abc9c;
        text-decoration: none;
    }

    footer a:hover {
        text-decoration: underline;
    }

    .aside-top {
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 10px; /* Adjust padding as needed */
        background-color: rgb(174, 204, 197); /* Updated to RGB color */
    }

    .brand-wrap {
        display: flex;
        align-items: center;
    }

    .logo {
        max-width: 100%;
        height: auto; /* Maintain aspect ratio */
        max-height: 50px; /* Adjust the height of the logo */
        object-fit: contain; /* Ensure the logo fits without distortion */
    }
</style>


